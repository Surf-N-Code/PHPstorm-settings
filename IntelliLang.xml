<application>
  <component name="LanguageInjectionConfiguration">
    <injection language="JSON" injector-id="twig">
      <display-name>Liquid JSON</display-name>
      <place><![CDATA[twigCustomStatement("schema")]]></place>
    </injection>
    <injection language="JavaScript" injector-id="twig">
      <display-name>Liquid Javascript</display-name>
      <place><![CDATA[twigCustomStatement("javascript")]]></place>
    </injection>
  </component>
</application>